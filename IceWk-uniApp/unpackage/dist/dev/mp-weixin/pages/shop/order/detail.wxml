<view class="data-v-2fe627fc"><block wx:for="{{orderInfo.goods_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="goods-info data-v-2fe627fc"><u-image vue-id="{{'19911544-1-'+index}}" width="150rpx" height="150rpx" src="{{item.cover_img}}" class="data-v-2fe627fc" bind:__l="__l"></u-image><view class="info-right data-v-2fe627fc"><text class="data-v-2fe627fc">{{item.title}}</text><view class="price data-v-2fe627fc"><text class="data-v-2fe627fc">{{"￥"+item.price}}</text><text class="num data-v-2fe627fc">{{"x "+orderInfo.goods_info[index].num}}</text></view></view></view></block><view class="cell data-v-2fe627fc"><view class="cell-item data-v-2fe627fc"><text class="data-v-2fe627fc">订单状态</text><text class="data-v-2fe627fc">{{orderInfo.status}}</text></view><view class="cell-item data-v-2fe627fc"><text class="data-v-2fe627fc">结算金额</text><text class="data-v-2fe627fc">{{orderInfo.total_money}}</text></view><view class="cell-item data-v-2fe627fc"><text class="data-v-2fe627fc">下单时间</text><text class="data-v-2fe627fc">{{$root.f0}}</text></view></view><view class="cell data-v-2fe627fc"><view class="cell-item data-v-2fe627fc"><text class="data-v-2fe627fc">联系人</text><text class="data-v-2fe627fc">{{orderInfo.address_info.name}}</text></view><view class="cell-item data-v-2fe627fc"><text class="data-v-2fe627fc">电话</text><text class="data-v-2fe627fc">{{orderInfo.address_info.moblie}}</text></view><view class="cell-item data-v-2fe627fc"><text class="data-v-2fe627fc">地址</text><text class="data-v-2fe627fc">{{orderInfo.address_info.region+orderInfo.address_info.address}}</text></view></view><block wx:if="{{orderInfo.express_company}}"><view class="cell data-v-2fe627fc"><view class="cell-item data-v-2fe627fc"><text class="data-v-2fe627fc">物流信息</text><text class="data-v-2fe627fc">{{orderInfo.express_company.label}}</text></view><view class="cell-item data-v-2fe627fc"><text class="data-v-2fe627fc">物流单号</text><text class="data-v-2fe627fc">{{orderInfo.express_number}}</text></view></view></block><block wx:if="{{orderInfo.status=='已完成'}}"><view class="cell data-v-2fe627fc"><view class="cell-item data-v-2fe627fc"><text class="data-v-2fe627fc">订单状态</text><text style="color:red;" class="data-v-2fe627fc">已完成</text></view></view></block><block wx:if="{{orderInfo.status=='待收货'}}"><view class="cell data-v-2fe627fc"><u-button style="margin:0 30rpx;" vue-id="19911544-2" shape="circle" type="success" data-event-opts="{{[['^click',[['submit']]]]}}" bind:click="__e" class="data-v-2fe627fc" bind:__l="__l" vue-slots="{{['default']}}">收货</u-button></view></block></view>